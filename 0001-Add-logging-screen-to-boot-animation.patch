+BootAnimation::Font text_font;
+int maxLines;
+int fd1 = -1;
+int fd2 = -1;
+int max_char;
+
+void init_fd() {
+    if (fd1 == -1) {
+        fd1 = android_get_control_file("/dev/kmsg");
+    }
+    //if (fd2 == -1) {
+    //    fd2 = android_get_control_file("/data/misc/recovery/last_log");
+    //}
+}
+
+void add_wrap(std::vector<std::string>& vec, std::string s) {
+    for (int i = 0; i < s.length(); i += max_char) {
+        vec.push_back(s.substr(i, max_char));
+    }
+}
+
+std::vector<std::string> read_mes(int fd) {
+    std::vector<std::string> messages;
+    if (fd == -1) {
+        messages.push_back("Error opening fd");
+        return messages;
+    }
+
+    lseek(fd, 0, SEEK_SET);
+    char buffer[256];
+    ssize_t bytesRead;
+    std::string line;
+    while ((bytesRead = read(fd, buffer, sizeof(buffer))) > 0) {
+        for (ssize_t i = 0; i < bytesRead; ++i) {
+            if (buffer[i] == '\n') {
+                messages.push_back(line);
+                line.clear();
+                if (messages.size() > maxLines) {
+                    messages.erase(messages.begin());
+                }
+            } else {
+                line += buffer[i];
+            }
+        }
+    }
+
+    if (bytesRead == -1 && messages.size() == 0) {
+        messages.push_back("Error reading from /dev/kmsg");
+    }
+
+    return messages;
+}
+
+std::vector<std::string> dmesg() {
+    std::vector<std::string> messages;
+    FILE* pipe = popen("dmesg -t", "r");
+    if (!pipe) {
+        messages.push_back("Error dmesg");
+        return messages;
+    }
+
+    char buffer[256];
+    while (fgets(buffer, sizeof(buffer), pipe) != NULL ){
+        add_wrap(messages, buffer);
+        while (messages.size() > maxLines) {
+            messages.erase(messages.begin());
+        }
+    }
+    pclose(pipe);
+    return messages;
+}
